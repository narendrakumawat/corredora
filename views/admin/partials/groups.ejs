<div class="row">
  <div class="col s2"></div>
  <div class="col s8">
    <table>
      <tbody>
        <% if(data.groups) {%>
          <% data.groups.forEach(group => {%>
            <tr>
              <td><%= group.group_name%></td>
              <td><%= group.succ_callback%></td>
              <td><%= group.fail_callback%></td>
            </tr>
          <%})%>
        <%}%>
      </tbody>
    </table>
  </div>
</div>