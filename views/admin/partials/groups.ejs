<div class="row">
  <div class="col s2"></div>
  <div class="col s8">
    <table>
      <thead>
        <th>Group Name</th>
        <th>Succuss callback</th>
        <th>Failure callback</th>
        <th>Action</th>
      </thead>
      <tbody>
        <% if(data.groups) {%>
          <% data.groups.forEach(group => {%>
            <tr>
              <td><a href="/admin/dashboard/group/<%=group.id%>"><%= group.group_name%></a></td>
              <td><%= group.succ_callback.substr(0,20)%>...</td>
              <td><%= group.fail_callback.substr(0,20)%>...</td>
              <td>
                <button type="submit" onclick="deleteRequest('/admin/dashboard/group/<%=group.id%>');">Delete</button>
              </td>
            </tr>
          <%})%>
        <%}%>
      </tbody>
    </table>
  </div>
</div>