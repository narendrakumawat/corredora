<div class="card ui-app__page-content">
  <div class="card-content">
    <div class="card-title headline">Users </div>
    <div class="card-body">
      <div class="row">
        <div class="col s12">
          <table>
            <thead>
              <th>Username</th>
              <th>Email</th>
              <th>User Attributes</th>
              <th>SAML Attributes</th>
              <th>Action</th>
            </thead>
            <tbody>
              <% if(data.users) {%>
                <% data.users.forEach(user => {%>
                  <tr>
                    <td><%= user.username%></td>
                    <td><%= user.email%></td>
                    <td style=" word-break: break-all;"><%= user.attributes%></td>
                    <td style=" word-break: break-all;"><%= user.saml_attributes%></td>
                    <td>
                      <button type="submit" class="validate waves-effect waves-light btn "  onclick="deleteRequest('/admin/dashboard/group/<%= data.group.id%>/user/<%=user.id%>')">Delete</button>
                    </td>
                  </tr>
                <%})%>
              <%}%>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>