<br><br><br><br>
<div class="row">
  <div class="col s3"></div>
  <form id="saveIDP" class="col s6" method="POST" action="/admin/dashboard/identity-provider">
    <div class="row">
      <div class="input-field col s12">
        <input id="sso_login_url" name="sso_login_url" type="text" class="validate">
        <label for="sso_login_url">SSO Login URL</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input  id="sso_logout_url" name="sso_logout_url" type="text" class="validate">
        <label for="sso_logout_url">SSO Logout URL</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s6">
        <select name="group_id" form="saveIDP">
          <option value="" disabled selected>Choose your option</option>
          <% data.groups.forEach( group => {%>
            <option value="<%=group.id%>|<%=group.group_name%>"><%= group.group_name%></option>
          <%})%>
        </select>
        <label>Group</label>
      </div>
      <div class="input-field col s6">
        <select name="nameid_policies" form="saveIDP">
          <option value="" disabled selected>Choose your option</option>
          <% Object.keys(data.nameIdPolicies).forEach( nameIdpolicy => {%>
            <option value="<%=data.nameIdPolicies[nameIdpolicy]%>"><%= nameIdpolicy%></option>
          <%})%>
        </select>
        <label>Name Id Policies</label>
      </div>
    </div>
    <div class="row">
      <div class="col s12">
        <label>Force Authentication</label>
        <div class="switch">
          <label>
            Off
            <input type="checkbox" name="force_authn">
            <span class="lever"></span>
            On
          </label>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col s12">
        <div class="row">
          <div class="input-field col s12">
            <textarea form="saveIDP" id="textarea" name="certificates" class="materialize-textarea"></textarea>
            <label for="textarea">Certificate</label>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="" type="submit" value="Save" class="validate waves-effect waves-light btn">
      </div>
    </div>
  </form>
</div>
      
