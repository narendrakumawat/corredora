<div class="row">
  <div class="col s3"></div>
  <% if(data.mapper) {%>
	  <form class="col s6" method="POST" action="/admin/dashboard/editMapper">
	    <div class="row">
	      <div class="input-field col s12">
	        <input id="group_id" type="text" name="group_id" value=<%=data.mapper.group_id%> class="validate">
	        <label for="group_id">Group Id</label>
	      </div>
	    </div>
	    <div class="row">
	      <div class="input-field col s12">
	        <input id="saml_attr" type="text" name="saml_attribute" value=<%=data.mapper.saml_attribute%> class="validate">
	        <label for="succ">Saml Attribute</label>
	      </div>
	    </div>
	    <div class="row">
	      <div class="input-field col s12">
	        <input id="user_attr" type="text" name="user_attribute" value=<%=data.mapper.user_attribute%> class="validate">
	        <label for="fail">User Attribute</label>
	      </div>
	    </div>
	    <div class="row">
	      <div class="input-field col s12">
	     	 	<input name="id" value=<%=data.mapper.id%> hidden/>
	        <input id="" type="submit" value="Save" class="validate waves-effect waves-light btn">
	      </div>
	    </div>
	  </form>
  <%}%>
</div>