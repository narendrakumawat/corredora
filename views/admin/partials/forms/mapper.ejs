<br><br>
<div class="row">
  <div class="col s3"></div>
  <form class="col s6" method="POST" action="/admin/dashboard/group/<%=data.group.id%>/mapper">
    <h4>Create Mappers</h4>
    <div class="row">
      <div class="input-field col s12">
        <input id="saml_attr" type="text" name="saml_attribute" class="validate">
        <label for="succ">Saml Attribute</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="user_attr" type="text" name="user_attribute" class="validate">
        <label for="fail">User Attribute</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="" type="submit" value="Save" class="validate waves-effect waves-light btn">
      </div>
    </div>
  </form>
</div>