<div class="card ui-app__page-content">
  <div class="card-content">
    <div class="card-title headline">Create Mappers </div>
    <div class="card-body">
      <div class="row">
        <div class="col s3"></div>
        <form class="col s6" method="POST" action="/admin/dashboard/group/<%=data.group.id%>/mapper">
          
          <div class="row">
            <div class="input-field col s12">
              <i class="material-icons prefix">person</i>
              <input id="saml_attr" type="text" name="saml_attribute" class="validate">
              <label for="saml_attr">Saml Attribute</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <i class="material-icons prefix">person_outline</i>
              <input id="user_attr" type="text" name="user_attribute" class="validate">
              <label for="user_attr">User Attribute</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <button id="" type="submit" class="validate waves-effect waves-light btn right">Save</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>